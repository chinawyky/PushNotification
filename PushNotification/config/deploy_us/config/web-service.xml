<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE beans PUBLIC "-//SPRING/DTD BEAN/EN" 
    "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="webservice-configuration" class="com.telenav.cserver.backend.util.WebServiceConfiguration">  	      
        <property name="serviceConfigMap">
	    	<map>
	    	 	<entry key="PUSH_NOTIFICATION">
                        <ref bean ="PUSH_NOTIFICATION_CONFIG"/>
                </entry>
	        	
				<entry key="USERNAME">
             			<ref bean ="USERNAME_CONFIG"/>
         		</entry>				
	    	</map>
		</property>
   </bean>    
      
   
	<bean id="USERNAME_CONFIG" class="com.telenav.cserver.backend.config.SimpleServiceConfig">
   		<property name="serviceUrl">
	    		<value>http://usermanagement-ws.mypna.com:8080/tnws/services/UserManagementService11</value>
		</property>
		<property name="webServiceItem">
	    		<ref bean="Default_ServiceItem"/>
		</property>
   </bean>
   
   <bean id="PUSH_NOTIFICATION_CONFIG" class="com.telenav.cserver.backend.config.SimpleServiceConfig">
        <property name="serviceUrl">
                <value>http://hqs-ws-01:8080/tnws/services/PushNotificationService</value>
        </property>
        <property name="webServiceItem">
                <ref bean="Default_ServiceItem"/>
        </property>
   </bean>
   
   
   
   <bean id="Default_ServiceItem" class="com.telenav.cserver.backend.config.WebServiceItem">  	      
        <property name="serviceName">
	    	<value>default</value>
		</property>
		<property name="webServiceMaxConnectionPerHost">
	    	<value>10</value>
		</property>
		<property name="webServiceMinimumPoolSize">
	    	<value>10</value>
		</property>
		<property name="webServiceMaximumPoolSize">
	    	<value>30</value>
		</property>
		<property name="webServiceConnectionTimeout">
	    	<value>2000</value>
		</property>
		<property name="webServiceTimeout">
	    	<value>5000</value>
		</property>
   </bean>
 
</beans>
